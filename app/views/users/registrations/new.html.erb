<div class="wrapper">
  <h3>SignUp</h3>
  <div id="formContent">
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
    <%= f.text_field :name, autofocus: true, autocomplete: "name",class: 'input-custom',placeholder: 'name' %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email",class: 'input-custom',placeholder: 'email'%>
    <%= f.password_field :password, autocomplete: "new-password",class: 'input-custom',placeholder: 'password' %>
    <br />
    <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password",class: 'input-custom',placeholder: 'password' %>
    <%= f.select :role, [["Admin","Admin"], ["Viewer","Viewer"]], { :include_blank => "Please select" },class: 'input-custom'  %>
    <%= f.submit "Sign Up" %>
    <%end%>
  </div>
</div>
<%#= render "users/shared/links" %>
